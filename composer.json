    "description": "PSR-7 and PSR-15 implemetation routing system based on nikic/fast-route.",
    "keywords": ["router", "dispatcher", "psr-7", "psr-15", "middleware", "routing"],
    "minimum-stability": "dev",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "dr0n1k",
            "email": "x.dronov@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Limbo\\Routing\\": ""
        }
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "limbo/container": "^0.1.1",
        "nikic/fast-route": "1.3.0",
        "psr/http-message": "^1.0.1",
        "psr/http-server-middleware": "^1.0.1",
        "psr/http-server-handler": "^1.0.1"
